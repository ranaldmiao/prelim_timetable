<IfModule mod_rewrite.c>
	RewriteEngine on
	RewriteRule ^([^/.]*)/([^/.]*)/([^/.]*)/$ index.php?h1=$1&h2=$2&h3=$3
	RewriteRule ^([^/.]*)/([^/.]*)/([^/.]*)$ index.php?h1=$1&h2=$2&h3=$3
	RewriteRule ^([^/.]*)/([^/.]*)/$ index.php?h1=$1&h2=$2
	RewriteRule ^([^/.]*)/([^/.]*)$ index.php?h1=$1&h2=$2
	RewriteRule ^([^/.]*)/$ index.php?h1=$1
</IfModule>

